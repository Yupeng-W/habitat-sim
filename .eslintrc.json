{
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "plugins": [
    "html"
  ],
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "strict": ["error", "global"],
    "curly": "warn",
    "prettier/prettier": "error"
  },
  "parser": "babel-eslint"
}
